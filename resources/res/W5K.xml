<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="360dp" android:layout_height="match_parent" android:layoutDirection="ltr">
    <RelativeLayout android:id="@+id/viewMain" android:background="@drawable/ksw_benz_zuodi" android:layout_width="175dp" android:layout_height="378dp" android:layout_marginTop="60dp" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toTopOf="0">
        <TextView android:textSize="14sp" android:textColor="#9b9897" android:id="@+id/tvTitle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="PASS AIR BAG" android:layout_marginStart="18dp"/>
        <CheckBox android:id="@+id/chkBag" android:background="@drawable/ksw_benz_chk_bag" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:button="@null" android:layout_alignTop="@+id/tvTitle" android:layout_alignBottom="@+id/tvTitle" android:layout_marginEnd="20dp" android:layout_alignParentEnd="true"/>
        <ImageView android:id="@+id/imgCar" android:background="@drawable/ksw_benz_dipanshengjiang" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@+id/tvTitle" android:layout_centerHorizontal="true"/>
        <CheckBox android:id="@+id/chkChassisRise" android:background="@drawable/ksw_benz_chk_control2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:button="@null" android:layout_below="@+id/imgCar" android:layout_centerHorizontal="true"/>
        <Button android:id="@+id/btnChassisRise" android:background="@null" android:clickable="false" android:layout_width="132dp" android:layout_height="40dp" android:button="@null" android:layout_below="@+id/imgCar" android:layout_centerHorizontal="true"/>
        <TextView android:textSize="14sp" android:textColor="#9b9897" android:gravity="center" android:id="@+id/tvEspStatus" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="ESP OFF" android:layout_below="@+id/chkChassisRise"/>
        <ImageButton android:id="@+id/btnEsp" android:background="@drawable/ksw_benz_btn_esp" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:button="@null" android:layout_below="@+id/tvEspStatus" android:layout_centerHorizontal="true"/>
        <CheckBox android:id="@+id/chkRadar" android:background="@drawable/ksw_benz_chk_radar" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:button="@null" android:layout_below="@+id/btnEsp" android:layout_centerHorizontal="true"/>
        <CheckBox android:id="@+id/chkRadar2" android:background="@drawable/ksw_benz_chk_control2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:button="@null" android:layout_below="@+id/chkRadar" android:layout_centerHorizontal="true"/>
        <Button android:id="@+id/btnRadar2" android:background="@null" android:clickable="false" android:layout_width="132dp" android:layout_height="40dp" android:button="@null" android:layout_below="@+id/chkRadar" android:layout_centerHorizontal="true"/>
        <ImageView android:id="@+id/imgFold" android:background="@drawable/ksw_benz_control_fold" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@+id/chkRadar2" android:layout_centerHorizontal="true"/>
        <ImageButton android:id="@+id/btnFoldLeft" android:background="@drawable/ksw_benz_btn_fold" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="-2dp" android:layout_below="@+id/imgFold" android:layout_marginStart="20dp"/>
        <ImageButton android:id="@+id/btnFoldRight" android:background="@drawable/ksw_benz_btn_fold" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="-2dp" android:layout_below="@+id/imgFold" android:layout_marginEnd="20dp" android:layout_alignParentEnd="true"/>
        <ImageView android:id="@+id/imgDashboard" android:background="@drawable/ksw_benz_shezhi" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@+id/btnFoldRight" android:layout_centerHorizontal="true"/>
        <CheckBox android:id="@+id/chkDashboardLeft" android:background="@drawable/ksw_benz_chk_control_left" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="2dp" android:button="@null" android:layout_below="@+id/btnFoldRight" android:layout_toStartOf="@+id/imgDashboard"/>
        <CheckBox android:id="@+id/chkDashboardRight" android:background="@drawable/ksw_benz_chk_control_right" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="2dp" android:button="@null" android:layout_below="@+id/btnFoldRight" android:layout_toEndOf="@+id/imgDashboard"/>
        <ImageButton android:id="@+id/btnDashboardLeft" android:background="@drawable/ksw_benz_btn_control_add" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="-8dp" android:layout_below="@+id/imgDashboard" android:layout_marginStart="25dp"/>
        <ImageButton android:id="@+id/btnDashboardRight" android:background="@drawable/ksw_benz_btn_control_add" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="-8dp" android:layout_below="@+id/imgDashboard" android:layout_marginEnd="25dp" android:layout_alignParentEnd="true"/>
    </RelativeLayout>
    <RelativeLayout android:id="@+id/viewDashboardRight" android:background="@drawable/ksw_benz_control_bk" android:visibility="gone" android:layout_width="214dp" android:layout_height="150dp" android:layout_marginBottom="55dp" android:layout_marginStart="144dp" app:layout_constraintBottom_toBottomOf="@+id/viewMain" app:layout_constraintStart_toStartOf="0">
        <ImageButton android:id="@+id/btnDashBoardRightBack" android:background="@drawable/ksw_benz_btn_dashboard_back" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentBottom="true"/>
        <ImageButton android:id="@+id/btnDashBoardRightSub" android:background="@drawable/ksw_benz_btn_dashboard_sub" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="30dp" android:layout_marginStart="5dp"/>
        <ImageButton android:id="@+id/btnDashBoardRightAdd" android:background="@drawable/ksw_benz_btn_dashboard_add" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="30dp" android:layout_marginEnd="5dp" android:layout_alignParentEnd="true"/>
    </RelativeLayout>
    <RelativeLayout android:id="@+id/viewDashboardLeft" android:background="@drawable/ksw_benz_control_bk" android:visibility="gone" android:layout_width="214dp" android:layout_height="150dp" android:layout_marginBottom="59dp" android:layout_marginStart="55dp" app:layout_constraintBottom_toBottomOf="@+id/viewMain" app:layout_constraintStart_toStartOf="0">
        <ImageButton android:id="@+id/btnDashBoardLeftBack" android:background="@drawable/ksw_benz_btn_dashboard_back" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentBottom="true"/>
        <ImageButton android:id="@+id/btnDashBoardLeftSub" android:background="@drawable/ksw_benz_btn_dashboard_sub" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="30dp" android:layout_marginStart="5dp"/>
        <ImageButton android:id="@+id/btnDashBoardLeftAdd" android:background="@drawable/ksw_benz_btn_dashboard_add" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="30dp" android:layout_marginEnd="5dp" android:layout_alignParentEnd="true"/>
    </RelativeLayout>
    <ImageButton android:id="@+id/hideBenzDashboard" android:background="@null" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginStart="360dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>
